<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>/WEB-INF/applicationContext.xml</param-value>
    </context-param>
    <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>dispatcher</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>Servletauthentiff</servlet-name>
        <servlet-class>servlets.Servletauthentiff</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>rechercheCours</servlet-name>
        <servlet-class>servlets.rechercheCours</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>rechercheEtudiant</servlet-name>
        <servlet-class>servlets.rechercheEtudiant</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletInscription1</servlet-name>
        <servlet-class>servlets.ServletInscription1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SupprimerEtudiant1</servlet-name>
        <servlet-class>servlets.SupprimerEtudiant1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servletsuppc</servlet-name>
        <servlet-class>servlets.Servletsuppc</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddCours</servlet-name>
        <servlet-class>servlets.AddCours</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dispatcher</servlet-name>
        <url-pattern>*.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servletauthentiff</servlet-name>
        <url-pattern>/Servletauthentiff</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>rechercheCours</servlet-name>
        <url-pattern>/rechercheCours</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>rechercheEtudiant</servlet-name>
        <url-pattern>/rechercheEtudiant</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletInscription1</servlet-name>
        <url-pattern>/ServletInscription1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SupprimerEtudiant1</servlet-name>
        <url-pattern>/SupprimerEtudiant1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servletsuppc</servlet-name>
        <url-pattern>/Servletsuppc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddCours</servlet-name>
        <url-pattern>/AddCours</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>redirect.jsp</welcome-file>
    </welcome-file-list>
</web-app>
